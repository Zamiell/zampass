{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "files": [
    "**"
  ],
  "enableFiletypes": [
    "*"
  ],
  "enableGlobDot": true,
  "ignorePaths": [
    ".git/**",
    "dist/**",
    "node_modules/**"
  ],
  "words": [
    "autocrlf",
    "clipboardy",
    "dbaeumer",
    "esbenp",
    "isaacscript",
    "pnpm",
    "sarisia",
    "technote",
    "Zamiell",
    "zampass"
  ]
}
